//컴퓨터공학과 20200769 박지호
public class Bank {

	public static void main(String[] args) {
		
		Account ac = new Account();
		
		try {
			ac.deposit(100);
		}
		catch(MalformedData m) {
			m.printStackTrace();
		}
		ac.show();
		try {
			ac.withdraw(50);
		}
		catch(MalformedData m) {
			m.printStackTrace();
		}
	}
}
