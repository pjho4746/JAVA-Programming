		ac.check();
		*/
		
		//배열
		Account[] accounts = new Account[5];
		for(int i=0; i<accounts.length; i++) {
			accounts[i] = new Account();
			try {
				accounts[i].deposit(100);
			}
			catch(MalformedData m) {
				m.printStackTrace();
			}
			accounts[i].check();
			try {
				accounts[i].withdraw(50);
			}
			catch(MalformedData m) {
				m.printStackTrace();
			}
			accounts[i].check();
		}
	}
}
