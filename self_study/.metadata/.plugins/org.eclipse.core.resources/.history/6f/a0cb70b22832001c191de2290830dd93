package Rectangle;

public class Rectangle {
	Point lt, rb;
	
	public Rectangle(Point left_top, Point right_bottom) {
		lt = left_top;
		rb = right_bottom;
	}
	public Rectangle(int x1, int y1, int x2, int y2) {
		//참조변수에 객체 할당
		lt = new Point(x1, y1);
		rb = new Point(X2, y2);
	}
	public int getWidth() {
		int dx = rb.getX() - lt.getX();
	}
	public int getHeight() {
		int dy = rb.getY() - rb.getX();
	}
	public show() {
		
	}
	
	
}
