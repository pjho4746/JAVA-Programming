import java.util.InputMismatchException;
import java.util.Scanner;
public class Bank {

	public static void main(String[] args) {
		/*
		Account ac = new Account();
		
		try {
			ac.deposit(100);
		}
		catch(MalformedData m) {
			m.printStackTrace();
		}
		ac.check();
		try {
			ac.withdraw(50);
		}
		catch(MalformedData m) {
			m.printStackTrace();
