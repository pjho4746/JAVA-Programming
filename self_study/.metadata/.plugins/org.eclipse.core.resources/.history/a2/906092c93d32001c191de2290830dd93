package Person2;

import Person1.Person;

public class UnivStudent extends Person{
	protected String major;
	
	public UnivStudent(String name, int age, String major) {
		super(name, age);
		this.major = major;
	}
	public void WhoAreYou() {
		super.WhoAreYou();
		System.out.println(major+"전공 대학생");
	}
	
}