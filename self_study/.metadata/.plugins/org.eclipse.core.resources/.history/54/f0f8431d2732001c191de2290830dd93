package Rectangle;

public class Point {
	private int x;
	private int y;
	
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public void SetXY(int x, int y) {
		this.x = x;
		this.x = y;
	}
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	public void show() {
		System.out.print("("+x+", "+y+")");
	}
	
}
