package hw.plygon;

public class Rectangle extends Polygon {
	int weight;
	int height;
	Point leftBottom;
	Point rightTop;
	
	//폭과 높이, 위치를 전달받음
	public Rectangle(int weight, int height, Point leftBottom, Point rightTop)
	{
		this.weight = weight;
		this.height = height;
		this.leftBottom = leftBottom;
		this.rightTop = rightTop;
	}

	double getArea()
	{
		
	}
	boolean isSquare()
	{
		
	}
}
