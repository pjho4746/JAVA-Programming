package hw.plygon;

public class Rectangle extends Polygon {
	int x2, y2;
	
	//폭과 높이, 위치를 전달받음
	public Rectangle(int x, int y, int w, int h, String Type, int x2, int y2) {
		super(x, y, w, h, Type);
		this.x2 = x2;
		this.y2 = y2;
	}
	
	boolean isSquare(int x, int y, int x2, int y2)
	{
		//leftTop(x, y)  rightBottom(x2, y2)라고 가정
		if(x>x2||y>y2) {
			
			return false;
		}
	}
	
	double getArea()
	{
		
	}
	
}
