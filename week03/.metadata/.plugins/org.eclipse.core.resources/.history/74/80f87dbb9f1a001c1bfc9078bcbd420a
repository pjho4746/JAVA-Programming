
public class Tv {
	//Tv의 속성
	boolean status;
	int channel;
	int volume;
	
	//Tv의 전원을 켜거나 끄는 기능을 하는 메서드
	void pushPower(){
		this.status = !status;
		return;
	}
	
	//Tv 채널을 높이는 기능을 하는 메서드
	void channelUp(){
		if(this.channel >5) {
			System.out.println("더 이상 채널이 없습니다.")
			this.channel = 0;
		}
		this.channel++;
		return;
	}
	//Tv 채널을 높이는 기능을 하는 메서드
	void channelDown(){
		this.channel--;
		return;
	}
	
}
